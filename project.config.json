{
  "miniprogramRoot": "miniprogram/",
  "cloudfunctionRoot": "cloudfunctions/",
  "appid": "wx6f41ba1a7d7bc51e",
  "projectname": "wx-earth-clean",
  "compileType": "miniprogram",
  "libVersion": "3.10.2",
  "setting": {
    "urlCheck": true,
    "es6": true,
    "postcss": true,
    "minified": true,
    "coverView": true,
    "useIsolateContext": true,
    "useCompilerModule": true,
    "disableUseStrict": true,
    "userConfirmedUseCompilerModuleSwitch": true,
    "packNpmManually": true,
    "packNpmRelationList": [
      {
        "packageJsonPath": "miniprogram/package.json",
        "miniprogramNpmDistDir": "miniprogram/miniprogram_npm"
      }
    ]
  },
  "packOptions": {
    "ignore": [
      { "type": "glob", "value": "miniprogram/node_modules/**" },
      { "type": "glob", "value": "**/*.map" },
      { "type": "glob", "value": "**/*.md" },
      { "type": "glob", "value": "miniprogram/assets/data/*.geojson" },
      { "type": "glob", "value": "miniprogram/libs/troika-three-text.min.js" },
      { "type": "glob", "value": "WinReference/**" }
    ]
  }
}
